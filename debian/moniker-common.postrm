#!/bin/sh

set -e

case $1 in
	purge)
		if (which deluser && getent passwd moniker) > /dev/null 2>&1; then
			deluser --system --quiet --backup-to /var/lib moniker
		fi	
		if (which delgroup && getent group moniker) > /dev/null 2>&1; then
			delgroup --system --quiet moniker
		fi
		;;
esac

